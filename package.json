{
  "name": "mockin",
  "version": "0.0.1",
  "description": "pseudo frontend-relational database",
  "main": "dist/index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --require './__test__/test_helper.js' './__test__/*_spec.js' --recursive",
