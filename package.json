  "license": "MIT",
  "bugs": {
    "url": "https://github.com/l2silver/mockin/issues"
  },
  "homepage": "https://github.com/l2silver/mockin#readme",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "i": "^0.3.4",
    "immutable": "^3.8.1",
    "mocha": "^2.4.5"
  }
}
